<% include ./header_admin.ejs %>

<div class="col-lg-12" style="padding-bottom:120px">
<table class="table table-striped table-bordered table-hover" id="dataTables-example">
<thead>
<tr align="center">
<th>Index</th>
<th>Name</th>
<th>Category Parent</th>
<th>Gender</th>
<th>Delete</th>
<th>Edit</th>
</tr>
</thead>
<tbody>
<% var index = 0;
if(listCate) {
    listCate.forEach(function(item) { 
        index++; %>
        <tr class="odd gradeX" align="center">
        <td><%= index %></td>
        <td><%= item.name %></td>
        <td class="center"><i class="fa fa-trash-o  fa-fw"></i><a href="/admin/delete/<%= item.id %>"> Delete</a></td>
        <td class="center"><i class="fa fa-pencil fa-fw"></i> <a href="/admin/edit-cate/<%= item.id %>">Edit</a></td>
        </tr>
        <% }); } %>
        </tbody>
        </table>
        </div>

        <% include ./footer_admin.ejs %>

