<% include ./header_admin.ejs %>

<div class="col-lg-12" style="padding-bottom:120px">
<table class="table table-striped table-bordered table-hover" id="dataTables-example">
<thead>
<tr align="center">
<th>Index</th>
<th>Username</th>
<th>Email</th>
<th>Level</th>
<th>Delete</th>
<th>Edit</th>
</tr>
</thead>
<tbody>
<% var index = 0;
if(listUser) {
    listUser.forEach(function(item) { 
        index++; %>
        <tr class="odd gradeX" align="center">
        <td><%= index %></td>
        <td><%= item.username %></td>
        <td><%= item.email %></td>
        <td><%= item.level %></td>
        <td class="center"><i class="fa fa-trash-o  fa-fw"></i><a href="/admin/user/delete/<%= item._id %>">Delete</a></td>
        <td class="center"><i class="fa fa-pencil fa-fw"></i> <a href="/admin/user/edit/<%= item._id %>">Edit</a></td>
        </tr>
        <% }); } %>
        </tbody>
        </table>
        </div>

        <% include ./footer_admin.ejs %>

